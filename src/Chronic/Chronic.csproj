<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net4</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\Chronic.xml</DocumentationFile>
    <Version>0.3.2</Version>
    <Description>A natural language date parser for .Net. A port of Ruby's chronic.</Description>
    <Authors>Robert Wilczynski</Authors>
    <PackageIconUrl>https://raw.github.com/PingmanTools/nChronic/master/doc/chronic.png</PackageIconUrl>
    <PackageLicenseUrl>https://github.com/PingmanTools/nChronic/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/PingmanTools/nChronic/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/PingmanTools/nChronic/</RepositoryUrl>
    <PackageTags>date time parsing DateTime chronic natural language</PackageTags>
    <PackageId>Chronic-netstandard</PackageId>
    <Company />
  </PropertyGroup>

</Project>
